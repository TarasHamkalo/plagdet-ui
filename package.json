{
  "name": "ux",
  "main": "electron.js",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "electron": "electron .",
    "electron-build": "ng build && electron .",
    "package-win": "electron-packager . plagdet-win --icon=public/favicon.ico --overwrite --asar --platform=win32 --arch=ia32  --prune=true --out=dist --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Plagdet\"",
    "package-linux": "electron-packager . plagdet-linux --overwrite --asar=true --platform=linux --arch=x64 --icon=public/favicon.ico --prune=true --out=dist",
    "package": "electron-forge package",
    "make-win": "electron-forge make --platform win32"

  },
  "private": true,
  "dependencies": {
